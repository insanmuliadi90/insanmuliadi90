---
layout: null
search: exclude
---

<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">

    {% for post in site.posts %}
    <url>
        <loc>{{site.url}}{{post.url}}</loc>
        <lastmod>
            {% if post.lastmod %}
            {{ post.lastmod | date: "%FT%T%:z" }}
            {% else %}
            {{site.time | date: "%FT%T%:z" }}
            {% endif %}
        </lastmod>
        
    </url>
    {% endfor %}

</urlset>
